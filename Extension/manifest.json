{
    "name": "FocusFlow",
    "description": "DIC FocusFlow Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/js/content.js"
            ]
        }
    ],
    "background": {
        "service_worker": "src/js/background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "static/images/icon.png"
    }
}